<%Post.where(is_published: false).order_by(published_at: :desc).each do |post|%>
	<%next unless post.feed%>
	<div class="panel panel-default" id="<%=post.published_at%>">
	  <div class="panel-body">
	    <%= link_to post.title, post.url, target: '_blank' %> - <i> published <%= time_ago_in_words(post.published_at) %> ago</i> by <b> <%=post.feed.title%> </b>
	  </div>
	  <br>
	</div>
<%end%>